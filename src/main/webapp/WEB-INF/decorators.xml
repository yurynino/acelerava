<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
    <decorator name="one-theme" page="one-theme.jsp">
        <pattern>/one/*</pattern>
    </decorator>
    <decorator name="two-theme" page="two-theme.jsp">
        <pattern>/two/*</pattern>
    </decorator>
    <decorator name="three-theme" page="three-theme.jsp">
        <pattern>/three/*</pattern>
    </decorator>
    <decorator name="report-theme" page="report-theme.jsp">
        <pattern>/reports/*</pattern>
    </decorator>
